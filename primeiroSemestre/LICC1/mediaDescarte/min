#include <stdio.h>

int minFunction(float a, float b, float c, float d);

int main(){
      float a, b, c, d;
      float min_1, min_2, min;
      scanf("%f %f %f %f", &a, &b, &c, &d);
      min = minFunction(a, b, c, d);

      float m;
      m = (a + b + c + d - min)/3
}

int minFunction(float a, float b, float c, float d){
      a < b? min_1 = a: min_1 = b;
      c < d? min_2 = c: min_2 = d;
      min_1 < min_2? min = min_1: min = min_2;
}
