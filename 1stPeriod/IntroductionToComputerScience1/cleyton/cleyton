#include <stdio.h>

int main(){
      //read the values tape
      int valuesTape[511];
      for (int i = 0; i < 512; i++){
            scanf("%i", &valuesTape[i]);
      }

      int n = 0;
      int a, b, c;
      while(n < 512){
            switch (valuesTape[n]){
                  case 0:
                        n = 512;
                        break;
                  case 1:
                        valuesTape[valuesTape[i+3]] = valuesTape[i+1] + valuesTape[i+2];
                        n = n + 4;
                        break;
                  case 2:
                        valuesTape[valuesTape[i+3]] = valuesTape[i+1] * valuesTape[i+2];
                        n = n + 4;
                        break;
                  case 3:
                        a = valuesTape[i+1];
                        b = valuesTape[i+2];
                        a < b? c == 1: c == 0;
                        break;
                  case 4:
                        break;
                  case 5:
                        break;
                  case 6:
                        break;
                  case 7:
                        break;
                  case 8:
                        break;
                  case 9:
                        break;


            }
      }

}
