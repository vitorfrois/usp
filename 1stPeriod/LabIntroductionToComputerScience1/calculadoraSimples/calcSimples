#include <stdio.h>

int main(){
      int a, b;
      char operation;
      scanf("%d %c %d", &a, &operation, &b);
      switch (operation){
            case '+':
                  printf("%d", a+b);
                  break;
            case '-':
                  printf("%d", a-b);
                  break;
            default:
                  printf("Caractere n√£o reconhecido.");
                  break;

      }

}
